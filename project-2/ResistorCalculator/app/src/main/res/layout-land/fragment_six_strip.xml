<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SixStripFragment">

    <Spinner
        android:id="@+id/spinner6_1"
        style="@style/spinnerStyle"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="4dp"
        app:layout_constraintEnd_toStartOf="@+id/spinner6_2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Spinner
        android:id="@+id/spinner6_2"
        style="@style/spinnerStyle"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="4dp"
        app:layout_constraintBottom_toBottomOf="@+id/spinner6_1"
        app:layout_constraintEnd_toStartOf="@+id/spinner6_3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/spinner6_1"
        app:layout_constraintTop_toTopOf="@+id/spinner6_1" />

    <Spinner
        android:id="@+id/spinner6_3"
        style="@style/spinnerStyle"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="4dp"
        app:layout_constraintBottom_toBottomOf="@+id/spinner6_2"
        app:layout_constraintEnd_toStartOf="@+id/spinner6_4"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/spinner6_2"
        app:layout_constraintTop_toTopOf="@+id/spinner6_2" />

    <Spinner
        android:id="@+id/spinner6_4"
        style="@style/spinnerStyle"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="4dp"
        app:layout_constraintBottom_toBottomOf="@+id/spinner6_3"
        app:layout_constraintEnd_toStartOf="@+id/spinner6_5"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/spinner6_3"
        app:layout_constraintTop_toTopOf="@+id/spinner6_3" />

    <Spinner
        android:id="@+id/spinner6_5"
        style="@style/spinnerStyle"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="4dp"
        app:layout_constraintBottom_toBottomOf="@+id/spinner6_4"
        app:layout_constraintEnd_toStartOf="@+id/spinner6_6"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/spinner6_4"
        app:layout_constraintTop_toTopOf="@+id/spinner6_4" />


    <Spinner
        android:id="@+id/spinner6_6"
        style="@style/spinnerStyle"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/spinner6_5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/spinner6_5"
        app:layout_constraintTop_toTopOf="@+id/spinner6_5" />


    <!--Results-->
    <TextView
        android:id="@+id/textView9"
        style="@style/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/resistance"
        app:layout_constraintBottom_toTopOf="@+id/resistance_result_6"
        app:layout_constraintEnd_toEndOf="@+id/resistance_result_6"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/resistance_result_6"
        app:layout_constraintTop_toBottomOf="@+id/spinner6_2" />

    <TextView
        android:id="@+id/resistance_result_6"
        style="@style/result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="5.00Ω"
        app:layout_constraintBottom_toTopOf="@+id/textView10"
        app:layout_constraintEnd_toEndOf="@+id/textView10"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <TextView
        android:id="@+id/textView10"
        style="@style/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tolerance"
        app:layout_constraintBottom_toTopOf="@+id/tolerance_result_6"
        app:layout_constraintEnd_toEndOf="@+id/tolerance_result_6"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/tolerance_result_6"
        app:layout_constraintTop_toBottomOf="@+id/resistance_result_6" />

    <TextView
        android:id="@+id/tolerance_result_6"
        style="@style/result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:text="± 0.25%"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />

    <TextView
        android:id="@+id/textView15"
        style="@style/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Minimum"
        app:layout_constraintBottom_toTopOf="@+id/min_result_6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinner6_4" />

    <TextView
        android:id="@+id/min_result_6"
        style="@style/result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="4.99kΩ"
        app:layout_constraintBottom_toTopOf="@+id/textView16"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <TextView
        android:id="@+id/textView16"
        style="@style/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Maximum"
        app:layout_constraintBottom_toTopOf="@+id/max_result_6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/min_result_6" />

    <TextView
        android:id="@+id/max_result_6"
        style="@style/result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="5.01kΩ"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView16" />

    <TextView
        android:id="@+id/textView17"
        style="@style/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:text="Tempco"
        app:layout_constraintBottom_toTopOf="@+id/temp_result_6"
        app:layout_constraintEnd_toEndOf="@+id/temp_result_6"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/temp_result_6"
        app:layout_constraintTop_toBottomOf="@+id/spinner6_5"
        app:layout_constraintVertical_chainStyle="packed" />

    <TextView
        android:id="@+id/temp_result_6"
        style="@style/result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="24dp"
        android:text="4.99kΩ"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView17" />


</android.support.constraint.ConstraintLayout>